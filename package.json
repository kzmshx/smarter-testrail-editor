    "lint": "npx eslint src",
    "fmt": "npx prettier --write src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ow-hirata/smarter-testrail-editor.git"
  },
  "author": "Kazumasa Hirata",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ow-hirata/smarter-testrail-editor/issues"
  },
  "homepage": "https://github.com/ow-hirata/smarter-testrail-editor#readme",
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "draft-js": "0.11.7",
    "draft-js-code": "0.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/preset-env": "7.15.4",
    "@babel/preset-react": "7.14.5",
