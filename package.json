    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "6.2.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "27.1.0",
    "prettier": "2.3.2",
    "style-loader": "3.2.1",
    "webpack": "5.52.0",
    "webpack-cli": "4.8.0"
  }
